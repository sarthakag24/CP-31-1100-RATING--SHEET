// HERE WHAT I DID THAT LIKE IF WE HAVE TO CHECK HOW MANY K SIZED STRING ARE THERE SO LAST K-1 ELEMENTS OF ALL STRING WOULD BE SAME AS MY REAL STRING NOW JUST HAVE TO PICK 
1 ELEMENT FROM THE REMAINING INTIITAL PART SO JUST CHECK HOW MAY UNIQUE ELEMENETS ARE THERE ..



#include<bits/stdc++.h>
using namespace std ; 


void solve ()
{ 
  int n ; 
  cin>>n  ;
  string s ; 
  cin >>s ; 

  set<char>st ; 

  long long ans = 0 ; 
  for (int i = 0 ; i < n ; i++)
  {
      st.insert(s[i]) ; 
     ans+= st.size() ; 

  }

cout<<ans<<endl ; 


}
int main ()
{
     int t   ; 
     cin>>t ; 
     while (t--)
     {solve() ; }
}
